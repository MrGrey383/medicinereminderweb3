{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/send-medicine-reminders",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/send-daily-reports",
      "schedule": "0 20 * * *"
    }
  ]
}